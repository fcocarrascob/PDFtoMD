    },
    {
      "type": "FormulaBlock",
      "raw": "fs_t_2 = 420 MPa",
      "id": "fs_t_2"
    },
    {
      "type": "FormulaBlock",
      "raw": "fs_c_2 = 360 MPa",
      "id": "fs_c_2"
    },
    {
      "type": "FormulaBlock",
      "raw": "phi_2 = 0.9",
      "id": "phi_2"
    },
    {
