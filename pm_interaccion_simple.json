{
  "version": 1,
  "blocks": [
    {
      "type": "TextBlock",
      "raw": "# Ejemplo de diagrama P\u2013M con compatibilidad de deformaciones\nColumna rectangular de hormig\u00f3n armado (ACI 318): c\u00e1lculo de puntos usando \u03d5 variable, esfuerzos de acero por deformaci\u00f3n y l\u00edmite de carga axial.",
      "id": "intro"
    },
    {
      "type": "TextBlock",
      "raw": "## Propiedades b\u00e1sicas",
      "id": "props"
    },
    {
      "type": "FormulaBlock",
      "raw": "fc = 28 MPa",
      "id": "fc"
    },
    {
      "type": "FormulaBlock",
      "raw": "fy = 420 MPa",
      "id": "fy"
    },
    {
      "type": "FormulaBlock",
      "raw": "Es = 200000 MPa",
      "id": "Es"
    },
    {
      "type": "FormulaBlock",
      "raw": "b = 300 mm",
      "id": "b"
    },
    {
      "type": "FormulaBlock",
      "raw": "h = 500 mm",
      "id": "h"
    },
    {
      "type": "FormulaBlock",
      "raw": "cobertura = 40 mm",
      "id": "cover"
    },
    {
      "type": "FormulaBlock",
      "raw": "diametro_barra = 16 mm",
      "id": "db"
    },
    {
      "type": "FormulaBlock",
      "raw": "beta1 = 0.85",
      "id": "beta1"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_cu = 0.003",
      "id": "eps_cu"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_y = fy / Es",
      "id": "eps_y"
    },
    {
      "type": "TextBlock",
      "raw": "## Geometr\u00eda derivada y \u00e1rea de acero",
      "id": "geom"
    },
    {
      "type": "FormulaBlock",
      "raw": "As_total = 8 * (pi * diametro_barra**2 / 4)",
      "id": "As_total"
    },
    {
      "type": "FormulaBlock",
      "raw": "As_t = As_total / 2",
      "id": "As_t"
    },
    {
      "type": "FormulaBlock",
      "raw": "As_c = As_total / 2",
      "id": "As_c"
    },
    {
      "type": "FormulaBlock",
      "raw": "d_t = h - cobertura - diametro_barra/2",
      "id": "d_t"
    },
    {
      "type": "FormulaBlock",
      "raw": "d_c = cobertura + diametro_barra/2",
      "id": "d_c"
    },
    {
      "type": "FormulaBlock",
      "raw": "P0 = 0.85 * fc * (b * h - As_total) + fy * As_total",
      "id": "P0"
    },
    {
      "type": "FormulaBlock",
      "raw": "Pn_cap = 0.8 * 0.65 * P0",
      "id": "Pn_cap"
    },
    {
      "type": "TextBlock",
      "raw": "## Profundidades de eje neutro a evaluar",
      "id": "c_vals"
    },
    {
      "type": "FormulaBlock",
      "raw": "c1 = 60 mm",
      "id": "c1"
    },
    {
      "type": "FormulaBlock",
      "raw": "c2 = 120 mm",
      "id": "c2"
    },
    {
      "type": "FormulaBlock",
      "raw": "c3 = 180 mm",
      "id": "c3"
    },
    {
      "type": "FormulaBlock",
      "raw": "c4 = 250 mm",
      "id": "c4"
    },
    {
      "type": "FormulaBlock",
      "raw": "c_balance = d_t * (eps_cu / (eps_cu + eps_y))",
      "id": "c_balance"
    },
    {
      "type": "FormulaBlock",
      "raw": "c_comp = 350 mm",
      "id": "c_comp"
    },
    {
      "type": "TextBlock",
      "raw": "## Resultados factorizados Pn\u2013Mn (sin llamadas a funciones externas)",
      "id": "resultados"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_t_1 = eps_cu * (d_t - c1) / c1",
      "id": "eps_t_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_c_1 = eps_cu * (c1 - d_c) / c1",
      "id": "eps_c_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "fs_t_1 = 420 MPa",
      "id": "fs_t_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "fs_c_1 = 120 MPa",
      "id": "fs_c_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "phi_1 = 0.9",
      "id": "phi_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "a_1 = beta1 * c1",
      "id": "a_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Cc_1 = 0.85 * fc * b * a_1",
      "id": "Cc_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Cs_1 = As_c * fs_c_1",
      "id": "Cs_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Ts_1 = As_t * fs_t_1",
      "id": "Ts_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Pn_nom_1 = phi_1 * (Cc_1 + Cs_1 + Ts_1)",
      "id": "Pn_nom_1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Pn1 = Pn_nom_1",
      "id": "Pn1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Mn1 = phi_1 * (Cc_1 * (a_1/2) + Cs_1 * d_c + Ts_1 * d_t)",
      "id": "Mn1"
    },
    {
      "type": "FormulaBlock",
      "raw": "Pn1_kN = Pn1 / 1000",
      "id": "Pn1_kN"
    },
    {
      "type": "FormulaBlock",
      "raw": "Mn1_kNm = Mn1 / 1000000",
      "id": "Mn1_kNm"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_t_2 = eps_cu * (d_t - c2) / c2",
      "id": "eps_t_2"
    },
    {
      "type": "FormulaBlock",
      "raw": "eps_c_2 = eps_cu * (c2 - d_c) / c2",
